                            function adtxBegin() {
                                var UserNe = $("#username").val();
                                var Passwd = $("#password").val();

                                if (UserNe == '' || Passwd == '') {
                                    if (UserNe == '' && Passwd == '') {
                                        $("#errormessages").show().html("Enter User Name and Password").addClass("alert alert-danger").delay(100000).fadeOut();
                                        return false;
                                    }
                                    if (UserNe == '') {
                                        $("#errormessages").show().html("Enter User Name").addClass("alert alert-danger").delay(100000).fadeOut();
                                        return false;
                                    }
                                    if (Passwd == '') {
                                        $("#errormessages").show().html("Enter Password").addClass("alert alert-danger").delay(100000).fadeOut();
                                    }
                                    return false;
                                }
                            }
